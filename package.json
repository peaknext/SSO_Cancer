{
  "name": "sso-cancer-care",
  "version": "1.0.0",
  "description": "SSO Cancer Care - Cancer Treatment Protocol Database",
  "private": true,
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "db:migrate": "npx prisma migrate dev --config prisma/prisma.config.ts",
    "db:seed": "npx prisma db seed --config prisma/prisma.config.ts",
    "db:studio": "npx prisma studio --config prisma/prisma.config.ts",
    "db:reset": "npx prisma migrate reset --config prisma/prisma.config.ts",
    "db:generate": "npx prisma generate --config prisma/prisma.config.ts"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "keywords": [
    "cancer",
    "protocol",
    "sso",
    "healthcare"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@prisma/client": "^7.4.1",
    "@types/node": "^25.3.0",
    "dotenv": "^16.5.0",
    "prisma": "^7.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.4.1"
  }
}
